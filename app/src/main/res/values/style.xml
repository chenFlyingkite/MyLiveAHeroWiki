<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="lahIcon">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/bg_press_sky</item>
    </style>


    <style name="common_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/t14dp</item>
    </style>
    <style name="common_text.white">
        <item name="android:textColor">#fff</item>
    </style>

    <style name="lahGo" parent="lahIcon">
        <item name="android:src">@drawable/ic_skip_next_black_48dp</item>
    </style>

    <style name="lahGo.W">
        <item name="android:src">@drawable/ic_skip_next_white_48dp</item>
    </style>

    <style name="lahGoTop" parent="lahGo">
        <item name="android:rotation">270</item>
    </style>

    <style name="lahGoBottom" parent="lahGo">
        <item name="android:rotation">90</item>
    </style>

    <style name="lahGoTopW" parent="lahGo.W">
        <item name="android:rotation">270</item>
    </style>
    <style name="lahGoTopW.t40">
        <item name="android:layout_width">@dimen/t40dp</item>
        <item name="android:layout_height">@dimen/t40dp</item>
    </style>

    <style name="lahGoBottomW" parent="lahGo.W">
        <item name="android:rotation">90</item>
    </style>
    <style name="lahGoBottomW.t40">
        <item name="android:layout_width">@dimen/t40dp</item>
        <item name="android:layout_height">@dimen/t40dp</item>
    </style>

</resources>